# .git-rc sources this file from your global `.gitconfig`

[color]
        branch  = auto
        diff    = auto
        grep    = auto
        status  = auto

[color "diff"]
       meta = yellow bold

[alias]
        ace  = add-core-excludes -g
        adt = attr-diff-type
        atd = attr-diff-type
        ae   = add-core-excludes -g
        aeg  = add-core-excludes -g
        ael   = add-core-excludes
        age  = add-core-excludes -g
        cdc  = config-diff-command
        cdcg = config-diff-command -g
        cdg  = config-diff-command -g
        cdgu = config-diff-command -gu
        cdt  = config-diff-command -t
        cdu  = config-diff-command -u
        cdtg = config-diff-command -tg
        ctg  = config-diff-command -tg
        ctgu = config-diff-command -tgu
        ctu  = config-diff-command -tu
        cds  = config diff.submodule
        cdsd = config diff.submodule diff
        cdsl = config diff.submodule log
        cgds  = config --global diff.submodule
        cgdsd = config --global diff.submodule diff
        cgdsl = config --global diff.submodule log
        cgsd = config --global diff.submodule diff
        cgsl = config --global diff.submodule log
        cf   = config
        cg   = config --global
        cfa  = config --add
        cga  = config --add --global
        cfd  = config --unset
        cgd  = config --unset --global
        cfda = config --unset-all
        cgda = config --unset-all --global
        cfl  = config --get-all
        cgl  = config --get-all --global
        cfs  = config --list
        cgs  = config --list --global
        cfu  = config --unset
        cgu  = config --unset --global
        cfua = config --unset-all
        cgua = config --unset-all --global
        cgdb = config --global init.defaultBranch
        cgdr = config --global clone.defaultRemoteName
        dbm = config --global init.defaultBranch main
        dnp = config diff.noprefix
        dnpg = config --global diff.noprefix
        dnpgt = config --global diff.noprefix true
        dnpgu = config --global --unset diff.noprefix
        dnpt = config diff.noprefix true
        dnpu = config --unset diff.noprefix
        dpg = config --global diff.noprefix
        dptg = config --global diff.noprefix true
        dpug = config --global --unset diff.noprefix
        dpt = config diff.noprefix true
        dpu = config --unset diff.noprefix
        dsm = config --global diff.submodule
        dsml = config --global diff.submodule log
        e = expand-alias
        ea = expand-alias
        gdb = config --global init.defaultBranch
        gdbm = config --global init.defaultBranch main
        if = config --global --get-all include.path
        ifa = config --global --add include.path
        gcf = config --global --get-all include.path
        gcfa = config --global --add include.path
        set-default-branch-main = config --global init.defaultBranch main
        sdb = set-default-branch-main
        sdbm = set-default-branch-main
        update-default-branch = set-default-branch-main
        udb = update-default-branch
        cdr = config --global clone.defaultRemoteName
        gdr = config --global clone.defaultRemoteName
        sdr = config --global clone.defaultRemoteName
        sdru = config --global clone.defaultRemoteName u
        cfi = config --get-all include.path
        fip = config --get-all include.path
        cgi = config --get-all --global include.path
        gip = config --get-all --global include.path
        global-config-file = config --get-all --global include.path
        global-gitignore = add-core-excludes -g
        ggi = global-gitignore
        gef = config --global --get-all core.excludesfile
        gefa = config --global --add core.excludesfile
        xf = config --global --get-all core.excludesfile
        xfa = config --global --add core.excludesfile
        rft = register-file-type
        rtg = register-file-type -g

        ip = config --global --get-all include.path
        ipa = config --global --add include.path
        ipu = config --global --unset-all include.path
        cu = config --unset
        cua = config --unset-all
        cug = config --unset --global
        cuga = config --unset-all --global

        cfaf = config core.attributesfile
        cgaf = config --global core.attributesfile
        cgg = config --global --get-all
        gaf = config --global core.attributesfile
        cfig = config core.excludesfile
        cfigs = config --get-all core.excludesfile
        cfef = config core.excludesfile
        cfefs = config --get-all core.excludesfile
        cgif = config --global core.excludesfile
        gif = config --global core.excludesfile
        cgig = config --global core.excludesfile
        gig = config --global core.excludesfile
        cgigs = config --global --get-all core.excludesfile
        cef = config --global --get-all core.excludesfile
        cefa = config --global --add core.excludesfile
        cefu = config --global --unset-all core.excludesfile
        ef = config --global --get-all core.excludesfile
        efa = config --global --add core.excludesfile
        efu = config --global --unset-all core.excludesfile
        cgef = config --global core.excludesfile
        cgefa = config --global --add core.excludesfile
        cfis = config core.excludesfile
        cgis = config --global core.excludesfile
        cfia = config --add core.excludesfile
        cgia = config --add --global core.excludesfile
        global-ignore-file = config --get-all --global core.excludesfile

        add-global-config-file = add-global-file include.path
        agi = add-global-ignore

        allow-pushes = config receive.denyCurrentBranch ignore
        aps = allow-pushes

        sat = set-attr-type.py
        sdt = set-attr-type.py -a diff
        sdm = set-attr-type.py -a diff,merge
        sdmt = set-attr-type.py -a diff,merge
        smt = set-attr-type.py -a merge
        uat = set-attr-type.py -u
        udt = set-attr-type.py -u -a diff
        udm = set-attr-type.py -u -a diff,merge
        umt = set-attr-type.py -u -a merge
        sid = set-id
        sgid = set-id -g
        gsid = set-id -g
        sidg = set-id -g
        sig = set-id -g
        sae = set-id
        gsae = set-id -g
        saeg = set-id -g
        gid = get-id
        gidg = get-id -g
        ggid = get-id -g
        fue = config user.email
        fun = config user.name
        fune = user
        gue = config --global user.email
        gun = config --global user.name

        sshc = ssh-command
        ue  = config user.email
        un  = config user.name
        une = user
        usr  = user
