_gea_precompute() {
    # Build caches for git_expand_alias batch operations.
    # Call before loops that invoke gea many times (e.g. diff-compgens).
    declare -gA _GEA_BASH_ALIASES=()
    declare -gA _GEA_GIT_ALIASES=()
    local _line
    while IFS= read -r _line; do
        local _name="${_line#alias }"
        _name="${_name%%=*}"
        local _val="${_line#*=\'}"
        _val="${_val%\'}"
        _GEA_BASH_ALIASES["$_name"]="$_val"
    done < <(alias)
    while IFS= read -r _line; do
        local _rest="${_line#alias.}"
        local _name="${_rest%% *}"
        local _val="${_rest#* }"
        _GEA_GIT_ALIASES["$_name"]="$_val"
    done < <(git config --get-regexp '^alias\.' 2>/dev/null)
    _GEA_PRECOMPUTED=1
}
export -f _gea_precompute

_gea_cleanup() {
    unset _GEA_BASH_ALIASES _GEA_GIT_ALIASES _GEA_PRECOMPUTED
}
export -f _gea_cleanup

git_expand_alias() {
    log() {
        return 0
    }
    if [ "$1" = "-v" ]; then
        shift
        log() {
            echo "$@" >&2
        }
    fi
    if [ $# -ne 1 ]; then
      echo "Usage: ${FUNCNAME[0]} [-v] <alias>" >&2
      return 1
    fi

    local pre=()
    local _aval=
    if [ -n "${_GEA_PRECOMPUTED-}" ] && [ -n "${_GEA_BASH_ALIASES[$1]+x}" ]; then
      _aval="${_GEA_BASH_ALIASES[$1]}"
    elif alias "$1" &>/dev/null; then
      _aval="$(alias "$1" 2>/dev/null)"
      _aval="${_aval#*=\'}"
      _aval="${_aval%\'}"
    fi
    if [ -n "$_aval" ]; then
      log "found alias $1" >&2
      set -- $_aval
      log "$# new args: $*"
      if [ "$1" = g ] || [ "$1" = git ] || [ "$1" = gn ]; then
        pre+=("$1")
        shift
      else
        echo "$@"
        return 0
      fi
    elif declare -F "$1" &>/dev/null; then
      log "found function $1" >&2
      local fline=
      local multiline=
      local _func_def
      _func_def="$(declare -f "$1" 2>/dev/null)" || true
      while IFS= read -r line; do
        [[ "$line" == "    "* ]] || continue
        line="${line#    }"
        log "function line: $line"
        # Check for Usage string (e.g. echo "Usage: func <args>" >&2)
        if [[ "$line" == *"Usage: "* ]] && [[ "$line" == *'>&2'* || "$line" == *'1>&2'* ]]; then
          local _usage="${line#*Usage: }"
          _usage="${_usage#* }"  # skip script/function name
          # Strip trailing quote + >&2 + semicolon (e.g. `" >&2;` or `' 1>&2`)
          _usage="${_usage%%\"*>&2*}"
          _usage="${_usage%%\'*>&2*}"
          _usage="${_usage%;}"
          if [ -n "$_usage" ]; then
            echo "$_usage"
            return 0
          fi
        fi
        if [ -z "$fline" ]; then
          fline="$line"
          log "fline: $fline"
        elif [ -n "$fline" ]; then
          multiline=1
        fi
      done <<< "$_func_def"
      if [ -n "$multiline" ]; then
        fline=
      fi
      log "got fline: $fline"
      if [ -z "$fline" ]; then
        # Multi-line function: try to read a description comment from the source file.
        # Uses extdebug to get file/line from declare -F. Callers can pre-enable
        # extdebug to avoid the per-call shopt toggle overhead.
        if shopt -q extdebug 2>/dev/null; then
          local _fn _src_line _src_file
          read -r _fn _src_line _src_file <<< "$(declare -F "$1" 2>/dev/null)"
          if [ -n "$_src_file" ] && [ -f "$_src_file" ] && [ -n "$_src_line" ]; then
            local _lines
            mapfile -t -s "$_src_line" -n 1 _lines < "$_src_file"
            local _next_line="${_lines[0]}"
            local _trimmed="${_next_line#"${_next_line%%[! ]*}"}"
            if [[ "$_trimmed" == "# "* ]]; then
              echo "# ${_trimmed#\# }"
              return 0
            fi
          fi
        fi
        return 1
      fi
      # Strip trailing "$@" from one-liner function body using bash builtins
      local _fline="$fline"
      _fline="${_fline% \"\$@\"}"
      set -- $_fline
      log "$# new args: $*"
      if [ "$1" = g ] || [ "$1" = git ]; then
        if [ $# -eq 1 ]; then
          echo "$1"
          return 0
        else
          pre+=("$1")
          shift
        fi
      else
        echo "$@"
        return 0
      fi
    else
      log "looking for cmd $1"
      local which="$(type -P "$1" 2>/dev/null)"
      if [ -n "$which" ]; then
        which="${which#$PWD/}"
        log "found command $1" >&2
        # Extract first meaningful comment, skipping uv metadata block
        local comment="$(sed -n '/^# \/\/\//,/^# \/\/\/$/d; /^# ./{p;q;}' "$which")"
        local out="$which"
        if [ -n "$comment" ]; then
          out="$out  $comment"
        fi
        echo "$out"
        return 0
      fi
    fi
    log "Looking up git alias: $*" >&2
    if [ "$1" = --no-pager ]; then
      pre+=("$1")
      shift
    fi
    local post=()
    if [ $# -gt 0 ]; then
      local git_alias="$1"; shift
      post=("$@")
      if [ -n "${_GEA_PRECOMPUTED-}" ] && [ -n "${_GEA_GIT_ALIASES[$git_alias]+x}" ]; then
        log "$git_alias -> ${_GEA_GIT_ALIASES[$git_alias]} (cached)"
        git_alias="${_GEA_GIT_ALIASES[$git_alias]}"
      else
        # Use git help to expand alias, parse with bash builtins (avoid head|perl)
        local _help_line
        _help_line="$(git help "$git_alias" 2>/dev/null | head -n1)"
        if [[ "$_help_line" == *"is aliased to '"*"'"* ]]; then
          local git_alias2="${_help_line#*is aliased to \'}"
          git_alias2="${git_alias2%\'*}"
          log "$git_alias -> $git_alias2"
          git_alias="$git_alias2"
        fi
      fi
      set -- $git_alias
      log "$# new args via git help: $*"
      if [[ $1 == "!"* ]]; then
        log "git shell config: $*"
        echo "$@"
        return 0
      else
        local pre_str=
        for arg in "${pre[@]}"; do
          pre_str="$pre_str${pre_str:+ }$arg"
        done
        echo "$pre_str" "$@" "${post[@]}"
      fi
    else
      local pre_str=
      for arg in "${pre[@]}"; do
        pre_str="$pre_str${pre_str:+ }$arg"
      done
      echo "$pre_str" "$@"
    fi
}
export -f git_expand_alias
defn gea git_expand_alias
defn geav git_expand_alias -v
