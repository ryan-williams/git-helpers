[alias]
  d    = diff
  d1   = diff-no-context
  dd   = diff --submodule=diff
  de   = diff --exit-code
  deq  = diff --exit-code --quiet
  dex  = diff --exit-code
  dqe  = diff --exit-code --quiet
  dsd  = diff --submodule=diff
  dl   = diff --submodule=log
  dsl  = diff --submodule=log
  dss  = diff --submodule=short
  db   = diff-branch-reflog
  dbr  = diff-branch-reflog
  dph  = diff HEAD^..HEAD
  dp2p = diff HEAD~2..HEAD^
  dc   = diff --cached
  dcd  = diff --cached --submodule=diff
  dcp  = diff --cached --submodule=diff
  dcq  = diff --cached --quiet
  dcss = diff --cached --submodule=short
  dcsw = diff --cached --numstat -w
  dcw  = diff --cached -w
  dcws = diff --cached -w --numstat
  df   = diff-filter
  dfm  = diff-filter M
  dg   = diff-gif.py
  dgif = diff-git.py
  dj   = diff-json.py
  ddf  = diff-filter
  dft  = difftool
  dh   = diff HEAD
  dh1  = diff HEAD^..HEAD
  dh2  = diff HEAD^2..HEAD
  dhw  = diff -w HEAD
  dn   = diff --name-status
  dno  = diff --name-only
  dnoa = diff-name-only-all
  dcno = diff --name-only --cached
  dnoc = diff --name-only --cached
  dns  = diff --name-status
  dp1 = diff-vs-parent 1
  dp2 = diff-vs-parent 2
  dq   = diff --quiet
  dqc  = diff --quiet --cached
  ds   = diff --numstat
  dsc  = diff --numstat --cached
  dcs  = diff --numstat --cached
  dsh  = diff --numstat HEAD
  dsw  = diff --numstat -w
  dws  = diff -w --numstat
  dsp  = diff --numstat HEAD^
  dsph = diff --numstat HEAD^..HEAD
  dst  = diff --stat
  dsct = diff --cached --stat
  dcst = diff --cached --stat
  dstc = diff --stat --cached
  dsth = diff --stat HEAD
  dstw = diff --stat -w
  dstp = diff --stat HEAD^
  dstph = diff --stat HEAD^..HEAD
  dsu  = diff-stat-parent
  dt   = diff-tree
  dtp  = diff-tree -p
  du   = diff -u
  dw   = diff -w
  dwc  = diff -w --cached
  dwh  = diff -w HEAD
  d-   = diff --
  dw-  = diff -w --
  dc-  = diff --cached --
  dcw- = diff --cached -w --
  ; pip install qmdx
  dx = diff-x
  dxc = diff-x -c
  dxcr = diff-x -cR
  dxcrr = diff-x -cr
  dxr = diff-x -R
  dxrr = diff-x -r
  dxw = diff-x -w
  dxwr = diff-x -wR
  dxwrr = diff-x -wr

  has-changes = is-dirty
  has-no-changes = is-clean
  hc = has-changes
  hnc = has-no-changes
  is-clean = diff --quiet HEAD
  is-clean-cached = diff --cached --quiet HEAD
  is-dirty = ! ! git is-clean
  is-dirty = ! ! git is-clean-cached
  ic = is-clean
  icc = is-clean-cached
  id = is-dirty
  idc = is-dirty-cached
  no-diff = diff --quiet HEAD

  nd = !git --no-pager diff
  ndc = nd --cached
  ndh = nd HEAD
  ndn = nd --name-only

  ; diff of diffs (git-didi - install via: pipx install git-didi)
  ddi = !git-didi
  dds = !git-didi stat
  ddsc = !git-didi stat --color=always
  ddp = !git-didi patch
  ddpc = !git-didi patch --color=always
  ddpq = !git-didi patch --quiet
  ddpqc = !git-didi patch --quiet --color=always
  ddc = !git-didi commits
  ddcc = !git-didi commits --color=always
