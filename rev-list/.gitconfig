[alias]
  rvl = rev-list
  rlp = rev-list --parents
  rlp1 = rev-list --parents -n1
  rla = rev-list --all
  rlc = rev-list --count
  rlfp = rev-list-first-parents
  ac = all-commits
  ah = all-hashes
  cc = count-commits
  tail = rev-list --max-parents=0 HEAD
  rvp = rev-parse
  commits-behind = rev-list --count
  cbh = commits-behind
  parents = !git-parents.py
  pts = !git-parents.py
  prnts = !git-parents.py
