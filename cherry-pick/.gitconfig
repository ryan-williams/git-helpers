[alias]
  cp   = cherry-pick
  cp1  = cherry-pick -m1
  cp2  = cherry-pick -m2
  cpa  = cherry-pick --abort
  cpc  = cherry-pick --continue
  cph  = cherry-pick-head
  cpn  = cherry-pick -n
  cppcd = cherry-pick-preserve-commit-date
  cps  = cherry-pick --skip
  cpsh = cherry-pick-show-head
  shch = cherry-pick-show-head
  acpc = add-and-cherry-pick-continue
