
[alias]
  rs = reset
  rsh1 = reset HEAD@{1}
  rsh2 = reset HEAD@{2}
  rsp = reset HEAD^
  rsp2 = reset HEAD~2
  rsp3 = reset HEAD~3
  rss = !git reset . && git submodule update --recursive && git status
  rp = reset -p
  rh = reset-hard
  rh1 = reset-hard HEAD@{1}
  rhh = reset --hard HEAD
  rhp = reset --hard HEAD^
  rhp2 = reset --hard HEAD~2
  rrs = reverse-reset
  rsf = checkout HEAD --
